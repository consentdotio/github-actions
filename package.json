{
	"name": "consent-github-actions",
	"version": "0.1.0",
	"private": true,
	"description": "A collection of reusable GitHub Actions for Consent projects",
	"author": "Consent",
	"license": "MIT",
	"engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
	"scripts": {
		"build": "pnpm -r run build",
		"test": "pnpm -r run test",
		"lint": "pnpm -r run lint",
		"fmt": "pnpm -r run fmt",
		"check-types": "pnpm -r run check-types",
		"clean": "pnpm -r exec rm -rf dist node_modules"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"typescript": "^5.7.2"
	}
}

